{
  "name": "quarry.team-vue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "gridsome build",
    "develop": "gridsome develop",
    "explore": "gridsome explore",
    "lint": "eslint \"@(src)/**/*.@(js|vue)\"",
    "prettier": "prettier --write \"@(src)/**/*.@(vue|js|scss)\" \"*.js\"",
    "prettier:validate": "prettier --check \"@(src)/**/*.@(js|vue|scss)\" \"*.js\""
  },
  "dependencies": {
    "@gridsome/source-wordpress": "^0.5.2",
    "aos": "^3.0.0-beta.6",
    "google-maps": "^4.2.3",
    "gridsome": "^0.7.0",
    "moment": "^2.24.0",
    "vue-flip": "^0.3.0",
    "vue-scrollto": "^2.17.1",
    "vue2-google-maps": "^0.10.7",
    "vuetify": "^2.1.13",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@mdi/font": "^4.7.95",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-gridsome": "^1.4.0",
    "husky": "^3.1.0",
    "node-sass": "^4.13.0",
    "prettier-check": "^2.0.0",
    "sass-loader": "^8.0.0",
    "vue-eslint-parser": "^7.0.0",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run prettier:validate && yarn run lint"
    }
  }
}
